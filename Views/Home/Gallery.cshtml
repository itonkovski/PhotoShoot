@{
    ViewData["Title"] = "TaraDemo - Gallery";
}

<section class="py-0 overflow-hidden">
    <div class="background-holder overlay overlay-2 parallax" style="background-image: url(../assets/images/main-header-1.jpg);"> </div>
    <!--/.background-holder-->
    <div class="container">
        <div class="row align-items-center justify-content-center text-center pt-8 pb-6" style="height: 50vh; min-height: 300px;" data-zanim-timeline="{}" data-zanim-trigger="scroll">
            <div class="col">
                <div class="overflow-hidden">
                    <h1 class="color-white fs-5 fs-lg-7" data-zanim='{"delay":0.1}'>Gallery</h1>
                </div>
            </div>
        </div>
        <!--/.row-->
    </div>
    <!--/.container-->
</section>
<section class="sortable pt-4 pb-0">
    <div class="container-fluid.px-0">
        <div class="row">
            <div class="col-lg-8 mb-4">
                <img src="~/assets/images/feature/8.jpg" alt="" />
            </div>
            <div class="col-lg-4">
                <div class="row">
                    <div class="col-lg-12 mb-4">
                        <img src="~/assets/images/feature/2.jpg" alt="" />
                    </div>
                    <div class="col-lg-12 mb-4">
                        <img src="~/assets/images/feature/3.jpg" alt="" />
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-6 mb-4">
                <img src="~/assets/images/feature/4.jpg" alt="" />
            </div>
            <div class="col-lg-6 mb-4">
                <img src="~/assets/images/feature/5.jpg" alt="" />
            </div>
        </div>
        <div class="row">
            <div class="col-lg-4">
                <div class="row">
                    <div class="col-lg-12 mb-4">
                        <img src="~/assets/images/feature/6.jpg" alt="" />
                    </div>
                    <div class="col-lg-12 mb-4">
                        <img src="~/assets/images/feature/7.jpg" alt="" />
                    </div>
                </div>
            </div>
            <div class="col-lg-8 mb-4">
                <img src="~/assets/images/feature/1.jpg" alt="" />
            </div>
        </div>
        <div class="row">
            <div class="col-lg-6 mb-4">
                <img src="~/assets/images/feature/9.jpg" alt="" />
            </div>
            <div class="col-lg-6 mb-4">
                <img src="~/assets/images/feature/10.jpg" alt="" />
            </div>
        </div>
        <!--/.row-->
    </div>
    <!--/.container-->
</section>